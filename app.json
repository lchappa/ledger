{
  "expo": {
    "name": "ledger",
    "slug": "ledger",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ledgerlogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/ledgerlogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ledgerlogo.png",
        "backgroundColor": "#000000"
      },
      "package": "com.anonymous.ledger",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
